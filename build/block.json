{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "create-block/liascript-markdown-block",
  "version": "0.1.0",
  "title": "Liascript Markdown Block",
  "category": "formatting",
  "icon": "media-code",
  "description": "Example block scaffolded with Create Block tool.",
  "example": {},
  "supports": {
    "html": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "textdomain": "liascript-markdown-block",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "attributes": {
    "content": {
      "type": "string",
      "default": ""
    },
    "postAuthor": {
      "type": "string",
      "default": "Post Author"
    },
    "iframeHeight": {
      "type": "number",
      "default": 600
    },
    "iframeSrc": {
      "type": "string",
      "default": "#"
    },
    "align": {
      "type": "string",
      "default": ""
    },
    "lia": {
      "type": "object",
      "default": {
        "search": "?data:text/plain;charset=utf-8;Content-Encoding=gzip;base64,",
        "course": "https://liascript.github.io/course/",
        "editor": "https://liascript.github.io/LiveEditor/?/show/code/"
      }
    }
  }
}